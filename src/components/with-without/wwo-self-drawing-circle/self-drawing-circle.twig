{% set schema = bolt.data.components["@bolt-components-self-drawing-circle"].schema %}

{% set attributes = create_attribute(attributes | default({})) %}

{% if dashSize %}
  {% set attributes = attributes.setAttribute("dashSize", dashSize) %}
{% endif %}

{% if speed %}
  {% set attributes = attributes.setAttribute("speed", speed) %}
{% endif %}

{% set attributes = attributes.setAttribute("animType", "self-drawing-circle") %}


<wwo-self-drawing-circle {{ attributes }}></wwo-self-drawing-circle>
